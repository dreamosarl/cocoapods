{
  "name": "ADM",
  "version": "1.0.0",
  "summary": "ADMStatic Library",
  "description": "ADMStatic is for mediating ads from different ad networks",
  "homepage": "http://www.admost.com",
  "authors": "Bora Met",
  "platforms": {
    "ios": "7.0"
  },
  "source": {
    "http": "https://github.com/dreamosarl/cocoapods/raw/master/Zip/ADM.zip"
  },
  "frameworks": [
    "Foundation",
    "UIKit",
    "CoreMedia"
  ],
  "ios": {
    "vendored_libraries": "libADMStaticLib.a",
    "source_files": "headers/*.h",
    "resources": [
      "ADMNativeViews/*.xib",
      "ADMNativeInterstitialViews/*.xib",
      "ADMGenericViews/*.xib",
      "ADMAdHandling.xcassets"
    ]
  },
  "preserve_paths": "libADMStaticLib.a",
  "requires_arc": true
}